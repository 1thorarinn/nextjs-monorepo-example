{
  "name": "blog-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "next export",
    "clean": "rimraf --no-glob ./.next ./out",
    "test:unit": "jest --config jest.config.js --color $@",
    "vercel-build": "cross-env NEXTJS_BUILD_TARGET=serverless next build",
    "start": "next start",
    "typecheck": "tsc -p ./ --noEmit",
    "deps:check": "npx npm-check-updates --dep prod,dev,optional",
    "deps:update": "npx npm-check-updates --dep prod,dev,optional -u"
  },
  "devDependencies": {
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "next-transpile-modules": "^6.0.0"
  },
  "dependencies": {
    "@optional-package-scope/foo": "^1.0.0",
    "cross-env": "^7.0.3",
    "next": "^10.0.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "license": "ISC"
}
